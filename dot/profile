export XDG_CONFIG_HOME=~/Desktop/configuration
export NVM_DIR=$XDG_CONFIG_HOME/nvm


alias tmux='TERM=xterm-256color tmux -f "$XDG_CONFIG_HOME"/tmux/tmux.conf'
alias vim="nvim"

export NVM_DIR="$HOME/Desktop/configuration/nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm


export PATH=$PATH:$HOME/Desktop/installation/flutter/bin:$HOME/.pub-cache/bin
